{
  "expo": {
    "name": "mobileplayercontrol",
    "slug": "mobileplayercontrol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/play_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7fffd4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/play_icon.png",
        "backgroundColor": "#7fffd4"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.kitkatdecode.playcontrol"
    },
    "web": {
      "favicon": "./assets/play_icon.png"
    }
  }
}
